	<pos number="6" type="PHR.">
		<paragraph number="1" name="a poem round and perfect as a star [Alex">
			<sg number="1">
				<word number="1">"a poem round and perfect as a star"</word>
			</sg>
			<sg number="2">
				<word number="1">Dichtung und Wahrheit</word>
			</sg>
			<sg number="3">
				<word number="1">furor poeticus</word>
			</sg>
			<sg number="4">
				<word number="1">"his virtues formed the magic of his song"</word>
			</sg>
			<sg number="5">
				<word number="1">"I do but sing because I must"</word>
			</sg>
			<sg number="6">
				<word number="1">"I learnt life from the poets"</word>
			</sg>
			<sg number="7">
				<word number="1">licentia vatum</word>
			</sg>
			<sg number="8">
				<word number="1">mutum est pictura poema</word>
			</sg>
			<sg number="9">
				<word number="1">"O for a muse of fire!"</word>
			</sg>
			<sg number="10">
				<word number="1">"sweet food of sweetly uttered knowledge"</word>
			</sg>
			<sg number="11">
				<word number="1">"the true poem is the poet's mind"</word>
			</sg>
			<sg number="12">
				<word number="1">Volk der Dichter und Denker</word>
			</sg>
			<sg number="13">
				<word number="1">"wisdom married to immortal verse"</word>
