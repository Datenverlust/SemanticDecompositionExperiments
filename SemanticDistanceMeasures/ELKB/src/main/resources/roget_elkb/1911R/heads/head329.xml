	<pos number="6" type="PHR.">
		<paragraph number="1" name="die Weltgeschichte ist das Weltergesicht">
			<sg number="1">
				<word number="1">die Weltgeschichte ist das Weltergesicht</word>
			</sg>
			<sg number="2">
				<word number="1">"earth is but the frozen echo of the silent voice of God"</word>
			</sg>
			<sg number="3">
				<word number="1">"green calm below</word>
				<word number="2">blue quietness above"</word>
			</sg>
			<sg number="4">
				<word number="1">"hanging in a golden chain this pendant World"</word>
			</sg>
			<sg number="5">
				<word number="1">"nothing in nature is unbeautiful"</word>
			</sg>
			<sg number="6">
				<word number="1">"silently as a dream the fabric rose"</word>
			</sg>
			<sg number="7">
