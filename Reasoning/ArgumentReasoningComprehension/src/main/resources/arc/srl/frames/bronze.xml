        <arg f="" n="0">they</arg>
        <rel f="">bronzed</rel>
        <arg f="" n="1">your seat in the corner</arg>
        <arg f="tmp" n="m">yet</arg>
      </example>
      <example name="All arguments, invented example" src="web usage" type="">
        <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
