        <role descr="metalsmith, bronzer" f="PAG" n="0">
          <vnrole vncls="9.9" vntheta="Agent"/>
        </role>
        <role descr="location, entity being bronzed" f="GOL" n="1">
          <vnrole vncls="9.9" vntheta="Destination"/>
        </role>
        <role descr="bronze itself, material used to cover" f="PPT" n="2">
          <vnrole vncls="9.9" vntheta="Theme"/>
        </role>
      </roles>
      <example name="Question, no mention of bronze material" src="web usage" type="">
