<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
  <predicate lemma="uppercase">
    <roleset id="uppercase.01" name="Use uppercase lettering">
      <aliases>
        <alias framenet="" pos="v" verbnet="">uppercase</alias>
        <alias framenet="" pos="n" verbnet="">uppercasing</alias>
      </aliases>
      <note>UPPERCASE-V NOTES: Added by Julia for EWT.</note>
      <note>UPPERCASING-N NOTES: Added by Julia for EWT.</note>
      <roles>
        <role descr="agent" f="pag" n="0"/>
        <role descr="lettering given in uppercase" f="ppt" n="1"/>
      </roles>
      <example name="uppercase-v" src="" type="">
        <text>Autocorrect just uppercased the word PICKLE. I don't know why. </text>
        <arg f="" n="0">Autocorrect</arg>
        <arg f="tmp" n="m">just</arg>
        <rel f="">uppercased</rel>
        <arg f="" n="1">the word PICKLE</arg>
      </example>
      <example name="uppercasing-n" src="" type="">
        <text>It was initially referred to as HoTMaiL with selective uppercasing .</text>
        <arg f="mnr" n="m">selective</arg>
        <rel f="">uppercasing</rel>
      </example>
    </roleset>
  </predicate>
</frameset>