<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
  <predicate lemma="hotspot">
    <roleset id="hotspot.01" name="turn into a wifi hotspot">
      <aliases>
        <alias framenet="" pos="v" verbnet="">hotspot</alias>
      </aliases>
      <note>HOTSPOT-V NOTES: Added by Julia</note>
      <roles>
        <role descr="agent" f="pag" n="0"/>
        <role descr="location becoming a hotspot" f="ppt" n="1"/>
      </roles>
      <example name="hotspot-v" src="" type="">
        <text> I rooted and hotspotted the phone relatively soon after buying it, seemingly without any problems</text>
        <arg f="" n="0">I</arg>
        <rel f="">hotspotted</rel>
        <arg f="" n="1">my phone</arg>
        <arg f="tmp" n="m">relatively soon after buying it</arg>
        <arg f="mnr" n="m">seemingly without any problems</arg>
      </example>
    </roleset>
    <roleset id="hotspot.02" name="create a web link in a certain location (the location being the hotspot)">
      <aliases>
        <alias framenet="" pos="v" verbnet="">hotspot</alias>
      </aliases>
      <note>HOTSPOT-V NOTES: Added by Julia</note>
      <roles>
        <role descr="web designer" f="pag" n="0"/>
        <role descr="location/word turned into a link" f="ppt" n="1"/>
        <role descr="what the link directs to" f="ppt" n="2"/>
      </roles>
      <example name="hotspot-v" src="" type="">
        <text>I hotspotted the points, hyperlinked to the relevant files, chose the lightbox option and added a caption.</text>
        <arg f="" n="0">I</arg>
        <rel f="">hotspotted</rel>
        <arg f="" n="1">the points</arg>
      </example>
    </roleset>
  </predicate>
</frameset>