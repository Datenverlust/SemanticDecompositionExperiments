<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
  <predicate lemma="retransfer">
    <roleset id="retransfer.01" name="resend">
      <aliases>
        <alias framenet="" pos="v" verbnet="">retransfer</alias>
        <alias framenet="" pos="n" verbnet="">retransfer</alias>
      </aliases>
      <note>RETRANSFER-V NOTES: Added by Julia.</note>
      <note>RETRANSFER-N NOTES: Added by Julia.</note>
      <roles>
        <note>Arg3 sent-from/"source" seems to be fairly uncommon. (from transfer.01-v)</note>
        <role descr="sender" f="PAG" n="0"/>
        <role descr="sent" f="PPT" n="1"/>
        <role descr="sent-to" f="GOL" n="2"/>
        <role descr="sent-from" f="DIR" n="3"/>
      </roles>
      <example name="retransfer-v" src="" type="">
        <text>[]Arms of U.S. origin]-1 cannot be retransferred *-1 without approval by the United States .</text>
        <arg f="mod" n="m">can</arg>
        <arg f="neg" n="m">not</arg>
        <rel f="">retransferred</rel>
        <arg f="" n="1">*-1</arg>
        <arg f="mnr" n="m">without approval by the United States</arg>
      </example>
    </roleset>
  </predicate>
</frameset>